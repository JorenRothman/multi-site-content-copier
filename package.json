{
    "name": "multi-content-copier",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "watch": {
        "build": {
            "patterns": [
                "assets"
            ],
            "extensions": "js",
            "ignore": "assets/multi-site-content-copier.min.js",
            "quiet": true,
            "runOnChangeOnly": true
        }
    },
    "scripts": {
        "dev": "npm-watch build",
        "build": "uglifyjs ./assets/multi-site-content-copier.js --compress --mangle --output ./assets/multi-site-content-copier.min.js ",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jorenrothman/multi-site-content-copier.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/jorenrothman/multi-site-content-copier/issues"
    },
    "homepage": "https://github.com/jorenrothman/multi-site-content-copier#readme",
    "dependencies": {
        "npm-watch": "^0.6.0",
        "uglify-js": "^3.8.0",
        "watch": "^1.0.2"
    }
}
